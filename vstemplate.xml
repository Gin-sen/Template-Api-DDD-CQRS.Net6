<VSTemplate Version="2.0.0" Type="ProjectGroup"
    xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
	<TemplateData>
		<Name>Multi-Project Template Sample</Name>
		<Description>An example of a multi-project template</Description>
		<Icon>Icon.ico</Icon>
        <ProjectType>CSharp</ProjectType>
        <LanguageTag>csharp</LanguageTag>
        <PlatformTag>windows</PlatformTag>
        <PlatformTag>linux</PlatformTag>
        <ProjectTypeTag>web</ProjectTypeTag>
        <ProjectTypeTag>library</ProjectTypeTag>
	</TemplateData>
	<TemplateContent>
		<ProjectCollection>
			<SolutionFolder Name="1.Interface">
				<ProjectTemplateLink ProjectName="$safeprojectname$.Api">
					My.DDD.CQRS.Temp6.Api\MyTemplate.vstemplate
				</ProjectTemplateLink>
			</SolutionFolder>
			<SolutionFolder Name="2.Business">
				<ProjectTemplateLink ProjectName="$safeprojectname$.Application">
					My.DDD.CQRS.Temp6.Application\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.Contracts">
					My.DDD.CQRS.Temp6.Contracts\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.Domain">
					My.DDD.CQRS.Temp6.Domain\MyTemplate.vstemplate
				</ProjectTemplateLink>
			</SolutionFolder>
			<SolutionFolder Name="3.Infrastructure">
				<ProjectTemplateLink ProjectName="$safeprojectname$.AzureTables">
					My.DDD.CQRS.Temp6.AzureTables\MyTemplate.vstemplate
				</ProjectTemplateLink>
			</SolutionFolder>
			<SolutionFolder Name="4.Common">
				<ProjectTemplateLink ProjectName="$safeprojectname$.AzureStorage">
					My.DDD.CQRS.Temp6.AzureStorage\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.CQRS">
					My.DDD.CQRS.Temp6.CQRS\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.Http.Bootstrap">
					My.DDD.CQRS.Temp6.Http.Bootstrap\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.Logging">
					My.DDD.CQRS.Temp6.Logging\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.Messaging">
					My.DDD.CQRS.Temp6.Messaging\MyTemplate.vstemplate
				</ProjectTemplateLink>
				<ProjectTemplateLink ProjectName="$safeprojectname$.RabbitMQ">
					My.DDD.CQRS.Temp6.RabbitMQ\MyTemplate.vstemplate
				</ProjectTemplateLink>
			</SolutionFolder>
		</ProjectCollection>
	</TemplateContent>
</VSTemplate>